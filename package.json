{
  "name": "react-redux-sass",
  "version": "0.1.0",
  "description": "React/Redux/Sass/Express Boilerplate",
  "engines": {
    "npm": ">=3.10.10",
    "node": ">=6.11.4"
  },
  "keywords": [
    "react",
    "redux",
    "sass",
    "boilerplate",
    "starter-kit"
  ],
  "author": "Aakash Goel <se.aakashgoel@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aakash-Goel/react-redux-sass.git"
  },
  "bugs": {
    "url": "https://github.com/Aakash-Goel/react-redux-sass/issues"
  },
  "homepage": "https://github.com/Aakash-Goel/react-redux-sass#readme",
  "scripts": {
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "shjs ./internals/scripts/createLogsDir.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "prebuild": "npm run build:clean",
    "prebuild:prod": "npm run build:clean",
    "build:clean": "rimraf ./dist",
    "start": "npm run lint && npm run build && npm run server",
    "build": "cross-env NODE_ENV=development webpack --config ./internals/webpack/webpack.dev.js --color --progress --display-error-details",
    "server": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 babel-node server/index.js",
    "start:prod": "npm run build:prod && npm run server:prod",
    "build:prod": "npm run lint && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color --progress --display-error-details",
    "server:prod": "cross-env NODE_ENV=production BABEL_DISABLE_CACHE=1 babel-node server/index.js",
    "lint": "npm run lint:js",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ext .jsx,.js --ignore-path .eslintignore --ignore-pattern internals/scripts"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.2",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "2.3.0",
    "compression": "1.7.1",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-redux-saga": "0.6.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "json-loader": "^0.5.7",
    "rimraf": "2.6.2",
    "shelljs": "0.7.8",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.1",
    "webpack-hot-middleware": "2.20.0",
    "webpack-merge": "4.1.1",
    "webpack-stats-plugin": "0.1.5"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bunyan": "^1.8.12",
    "bunyan-rotating-file-stream": "^1.6.3",
    "classnames": "2.2.5",
    "cross-env": "^5.1.1",
    "express": "4.16.2",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "ip": "^1.1.5",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0"
  }
}
